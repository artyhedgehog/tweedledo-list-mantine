{
  "name": "tweedledo-list-mantine",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:clean": "rm -rf ./dist && npm run build",
    "preview": "vite preview",
    "preview:clean": "killall node ; npm run build:clean && npm run preview &",
    "typecheck": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint . --cache",
    "eslint:fix": "eslint . --cache --fix",
    "stylelint": "stylelint '**/*.css' --cache",
    "stylelint:fix": "stylelint '**/*.css' --cache --fix",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "cy:test": "npm run preview:clean && npm run cy:run",
    "test": "npm run typecheck && npm run prettier:write && npm run lint:fix && npm run vitest",
    "test:full": "npm run test && npm run cy:test",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@mantine/core": "8.3.10",
    "@mantine/form": "^8.3.10",
    "@mantine/hooks": "8.3.10",
    "@mantine/notifications": "^8.3.10",
    "@tabler/icons-react": "3.36.0",
    "classnames": "2.5.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "todomvc-app-css": "2.4.3",
    "todomvc-common": "1.0.5",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-themes": "^10.0.0",
    "@storybook/react": "^10.0.0",
    "@storybook/react-vite": "^10.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "cypress": "3.8.3",
    "eslint": "^9.37.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^10.0.0",
    "stylelint": "^16.25.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-html": "^3.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.0"
  },
  "packageManager": "npm@11.6.2"
}
